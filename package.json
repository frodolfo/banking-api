    "morgan": "^1.10.0",
    "pg": "^8.8.0",
    "swagger-autogen": "^2.21.5",
    "swagger-ui-express": "^4.5.0"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4"
  }
